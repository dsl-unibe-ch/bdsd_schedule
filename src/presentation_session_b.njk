---
title: Morning Poster Session
layout: layouts/bdsd
date: Last Modified
---
<a class="my-4 font-semibold" href="/">&lt; Back to Main Schedule</a>

<h2 class="my-4 font-semibold">Afternoon Presentation Session</h2>

<table class="mx-auto min-w-[80%]">
  <thead>
    <tr class="bg-purple-300 text-lg">
      <th>Time</th>
      <th>Presentation</th>
      <th>Room</th>
    </tr>
  </thead>
  <tbody>
    {% for item in schedule_array -%}{% if item.belongs_to == "presentation_session_b" -%}
    <tr data-id="{{ item.id }}" data-belongs-to="{{ item.belongs_to }}" class="odd:bg-purple-100 even:bg-white odd:print:bg-slate-100">
      <td class="px-2 py-1.5">{{ item.time_start }}&ndash;{{ item.time_end }}</td>
      <td class="px-2 py-1.5">
        <div class="flex flex-col">
          <div class="font-semibold">
            {% if item.children | length %}
            <a href="/{{ item.type }}">{{ item.title }}</a>
            {% else %}
            {{ item.title }}
            {% endif %}
          </div>
          {%- if item.presented_by %}<div class="text-slate-900 text-sm">{{ item.presented_by }}</div>{% endif %}
        </div>
      </td>
      <td class="px-2 py-1.5">{{ item.room }}</td>
    </tr>
    {%- endif %}{%- endfor %}
  </tbody>
</table>